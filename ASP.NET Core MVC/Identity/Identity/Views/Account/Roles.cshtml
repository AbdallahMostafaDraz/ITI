@model IEnumerable<IdentityRole>

<h1>All Roles</h1>
<div class="row">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
            </tr>           
        </thead>
        <tbody>
            @foreach(var role in Model)
            {
                <tr>
                    <td>@role.Id</td>
                    <td>@role.Name</td>
                </tr>
            }
        </tbody>
    </table>
</div>